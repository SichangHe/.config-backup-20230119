{
	"3th header": {
		"prefix": "h3",
		"body": "### $0"
	},
	"4th header": {
		"prefix": "h4",
		"body": "#### $0"
	},
	"5th header": {
		"prefix": "h5",
		"body": "##### $0"
	},
	"6th header": {
		"prefix": "h6",
		"body": "###### $0"
	},
	"code": {
		"prefix": "code",
		"body": [
			"```$1",
			"$0",
			"```"
		],
		"description": "create code block"
	},
	"add math subscript": {
		"prefix": "sub",
		"body": "_{$0}",
		"description": "add `_{}`"
	},
	"create bmatrix": {
		"prefix": "bmt",
		"body": [
			"\\begin{bmatrix}",
			"    $0",
			"\\end{bmatrix}"
		],
		"description": "create a bmatrix"
	},
	"left and right parenthesis": {
		"prefix": "leftp",
		"body": [
			"\\left(",
			"    $0",
			"\\right)"
		],
		"description": "create left right parenthesis pair"
	},
	"left and right brackets": {
		"prefix": "leftk",
		"body": [
			"\\left[",
			"    $0",
			"\\right]"
		],
		"description": "create left right brackets pair"
	},
	"left and right braces": {
		"prefix": "leftc",
		"body": [
			"\\left\\{",
			"    $0",
			"\\right\\\\}"
		],
		"description": "create left right braces pair"
	},
	"fraction": {
		"prefix": "frac",
		"body": "\\frac{$0}{}",
		"description": "create a fraction"
	},
	"braces": {
		"prefix": "brace",
		"body": [
			"\\{",
			"    $0",
			"\\\\}"
		],
		"description": "a pair of braces"
	},
	"rank": {
		"prefix": "rank",
		"body": "\\text{rank}($0)"
	},
	"nullity": {
		"prefix": "nullity",
		"body": "\\text{nullity}($0)"
	},
	"1 to n": {
		"prefix": "oneton",
		"body": "$1_1,\\cdots,$1_n$0"
	},
	"1 to m": {
		"prefix": "onetom",
		"body": "$1_1,\\cdots,$1_m$0"
	},
	"norm with double bar": {
		"prefix": "norm",
		"body": "\\lVert $0\\rVert"
	},
	"left and right angle": {
		"prefix": "angle",
		"body": "\\langle $0\\rangle"
	}
}